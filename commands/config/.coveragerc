[run]
source = src,app
branch = True
omit = 
    */tests/*
    */test_*.py
    */*_test.py
    */conftest.py
    */__pycache__/*
    */venv/*
    */.venv/*
    */migrations/*
    */config/*
    */scripts/*

[report]
exclude_lines = 
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    
ignore_errors = True
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml