{
  "name": "blacklist-management-system",
  "version": "1.0.1393",
  "description": "Enterprise Threat Intelligence Platform with Real-time Dashboard",
  "main": "main.py",
  "scripts": {
    "test": "pytest tests/ -v",
    "start": "python scripts/run_with_credentials.py",
    "build": "docker build -t blacklist:latest .",
    "lint": "python -m flake8 src/ --count --select=E9,F63,F7,F82 --show-source --statistics",
    "format": "black src/ tests/ && isort src/ tests/",
    "clean": "find . -type d -name '__pycache__' -exec rm -rf {} + 2>/dev/null || true",
    "dev": "FLASK_ENV=development python scripts/run_with_credentials.py",
    "docker:build": "docker build -t registry.jclee.me/blacklist:latest .",
    "docker:push": "docker push registry.jclee.me/blacklist:latest",
    "docker:run": "docker run -p 32542:2542 registry.jclee.me/blacklist:latest",
    "deploy": "npm run docker:build && npm run docker:push",
    "version": "python scripts/auto_version_manager.py"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCLEE94/blacklist.git"
  },
  "keywords": [
    "threat-intelligence",
    "blacklist",
    "security",
    "flask",
    "python",
    "docker",
    "kubernetes",
    "cncf"
  ],
  "author": "JC Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JCLEE94/blacklist/issues"
  },
  "homepage": "https://blacklist.jclee.me",
  "engines": {
    "python": ">=3.9"
  },
  "dependencies": {},
  "devDependencies": {}
}